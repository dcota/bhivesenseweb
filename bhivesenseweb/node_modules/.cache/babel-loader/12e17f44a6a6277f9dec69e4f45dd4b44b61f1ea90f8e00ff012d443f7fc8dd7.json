e(\"noHivesInApiary\"),\n            type: \"warn\",\n            duration: 3000,\n            speed: 500\n          });\n        } else {\n          for (let i = 0; i < data.length; i++) {\n            let today = new Date();\n            let lastDate = new Date(data[i].date.toLocaleString(\"sv-SE\", {\n              timeZone: \"Atlantic/Azores\"\n            }));\n            if (today.getDay() == lastDate.getDay() && today.getMonth() == lastDate.getMonth()) {\n              let tempArray = [];\n              /*let arrDate =\r\n                    lastDate.getFullYear() +\r\n                    \"-\" +\r\n                    (lastDate.getMonth() + 1) +\r\n                    \"-\" +\r\n                    lastDate.getDate();*/\n              let time = lastDate.getHours() + \":\" + lastDate.getMinutes();\n              tempArray.push(time);\n              let arrTempIn = data[i].ti;\n              tempArray.push(arrTempIn);\n              this.chartData.push(tempArray);\n              console.log(tempArray);\n            }\n          }\n\n          /*let lastDate = new Date(\r\n                                                                                                    devices[0].data.date.toLocaleString(\"sv-SE\", {\r\n                                                                                                      timeZone: \"Atlantic/Azores\",\r\n                                                                                                    })\r\n                                                                                                  );\r\n                                                                                                  let sdConv = this.padTo2Digits(\r\n                                                                                                    lastDate.toLocaleDateString(\"sv-SE\", {\r\n                                                                                                      year: \"numeric\",\r\n    