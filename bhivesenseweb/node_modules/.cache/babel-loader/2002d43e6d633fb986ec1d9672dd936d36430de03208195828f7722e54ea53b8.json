{"ast":null,"code":"import en from \"../assets/en.js\";\nimport pt from \"../assets/pt.js\";\nimport { mapGetters } from \"vuex\";\nimport { IS_USER_AUTHENTICATED_GETTER } from \"../store/storeconstants\";\nexport default {\n  name: \"Modal\",\n  mixins: [en, pt],\n  props: [\"details\"],\n  data() {\n    const lang = localStorage.getItem(\"lang\") || \"pt\";\n    return {\n      lang: lang,\n      altImg: require(\"../assets/avatar.png\")\n    };\n  },\n  computed: {\n    ...mapGetters(\"auth\", {\n      isAuthenticated: IS_USER_AUTHENTICATED_GETTER,\n      auth: localStorage.getItem(\"auth\")\n    })\n  },\n  mounted() {},\n  methods: {\n    close() {\n      this.$emit(\"close\");\n    },\n    edit(id) {\n      localStorage.setItem(\"interventiontoedit\", id);\n      this.$emit(\"edit\");\n    },\n    done(id) {\n      localStorage.setItem(\"interventiontoedit\", id);\n      this.$emit(\"done\");\n    },\n    cancel(id) {\n      localStorage.setItem(\"interventiontoedit\", id);\n      this.$emit(\"delete\");\n    },\n    translate(prop) {\n      return this[this.lang][prop];\n    }\n  }\n};","map":{"version":3,"mappings":"AAyNC,OAAOA,EAAC,MAAO,iBAAiB;AAChC,OAAOC,EAAC,MAAO,iBAAiB;AAChC,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,4BAA2B,QAAS,yBAAyB;AACtE,eAAe;EACdC,IAAI,EAAE,OAAO;EACbC,MAAM,EAAE,CAACL,EAAE,EAAEC,EAAE,CAAC;EAChBK,KAAK,EAAE,CAAC,SAAS,CAAC;EAClBC,IAAI,GAAG;IACN,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAO,CAAC,MAAM,KAAK,IAAI;IACjD,OAAO;MACNF,IAAI,EAAEA,IAAI;MACVG,MAAM,EAAEC,OAAO,CAAC,sBAAsB;IACvC,CAAC;EACF,CAAC;EACDC,QAAQ,EAAE;IACT,GAAGX,UAAU,CAAC,MAAM,EAAE;MACrBY,eAAe,EAAEX,4BAA4B;MAC7CY,IAAI,EAAEN,YAAY,CAACC,OAAO,CAAC,MAAM;IAClC,CAAC;EACF,CAAC;EACDM,OAAO,GAAG,CAAC,CAAC;EACZC,OAAO,EAAE;IACRC,KAAK,GAAG;MACP,IAAI,CAACC,KAAK,CAAC,OAAO,CAAC;IACpB,CAAC;IACDC,IAAI,CAACC,EAAE,EAAE;MACRZ,YAAY,CAACa,OAAO,CAAC,oBAAoB,EAAED,EAAE,CAAC;MAC9C,IAAI,CAACF,KAAK,CAAC,MAAM,CAAC;IACnB,CAAC;IACDI,IAAI,CAACF,EAAE,EAAE;MACRZ,YAAY,CAACa,OAAO,CAAC,oBAAoB,EAAED,EAAE,CAAC;MAC9C,IAAI,CAACF,KAAK,CAAC,M