{"ast":null,"code":"import en from \"../assets/en.js\";\nimport pt from \"../assets/pt.js\";\nexport default {\n  name: \"Modal\",\n  mixins: [en, pt],\n  data: function () {\n    const lang = localStorage.getItem(\"lang\") || \"pt\";\n    return {\n      lang: lang,\n      option: \"\"\n    };\n  },\n  methods: {\n    close() {\n      this.$emit(\"close\");\n    },\n    unassign() {\n      //this.$emit(\"unassign\");\n      this.option = alert(this.option);\n    },\n    translate(prop) {\n      return this[this.lang][prop];\n    },\n    handleChange(event) {\n      if (event.target.value == \"1\") this.option = true;\n      if (event.target.value == \"2\") this.option = false;\n    }\n  }\n};","map":{"version":3,"mappings":"AAiLE,OAAOA,EAAC,MAAO,iBAAiB;AAChC,OAAOC,EAAC,MAAO,iBAAiB;AAChC,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,MAAM,EAAE,CAACH,EAAE,EAAEC,EAAE,CAAC;EAChBG,IAAI,EAAE,YAAY;IAChB,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAO,CAAC,MAAM,KAAK,IAAI;IACjD,OAAO;MACLF,IAAI,EAAEA,IAAI;MACVG,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,KAAK,GAAG;MACN,IAAI,CAACC,KAAK,CAAC,OAAO,CAAC;IACrB,CAAC;IACDC,QAAQ,GAAG;MACT;MACA,IAAI,CAACJ,MAAK,GAAIK,KAAK,CAAC,IAAI,CAACL,MAAM,CAAC;IAClC,CAAC;IACDM,SAAS,CAACC,IAAI,EAAE;MACd,OAAO,IAAI,CAAC,IAAI,CAACV,IAAI,CAAC,CAACU,IAAI,CAAC;IAC9B,CAAC;IACDC,YAAY,CAACC,KAAK,EAAE;MAClB,IAAIA,KAAK,CAACC,MAAM,CAACC,KAAI,IAAK,GAAG,EAAE,IAAI,CAACX,MAAK,GAAI,IAAI;MACjD,IAAIS,KAAK,CAACC,MAAM,CAACC,KAAI,IAAK,GAAG,EAAE,IAAI,CAACX,MAAK,GAAI,KAAK;IACpD;EACF;AACF,CAAC","names":["en","pt","name","mixins","data","lang","localStorage","getItem","option","methods","close","$emit","unassign","alert","translate","prop","handleChange","event","target","value"],"sourceRoot":"","sources":["C:\\Users\\duart\\OneDrive - PGA\\Ambiente de Trabalho\\bhivesenseweb\\bhivesenseweb\\src\\components\\ModalUnassign.vue"],"sourcesContent":["<template>\r\n  <transition name=\"modal-fade\">\r\n    <div class=\"modal-backdrop\">\r\n      <div\r\n        class=\"modal\"\r\n        role=\"dialog\"\r\n        aria-labelledby=\"modalTitle\"\r\n        aria-describedby=\"modalDescription\"\r\n      >\r\n        <header class=\"modal-header\" id=\"modalTitle\">\r\n          <slot name=\"header\"> {{ translate(\"cancelTitleModal\") }} </slot>\r\n        </header>\r\n\r\n        <section class=\"modal-body\" id=\"modalDescription\">\r\n          <slot name=\"body\"> {{ translate(\"modalUnassignText\") }} </slot>\r\n          <p></p>\r\n          <slot name=\"body\"> {{ translate(\"modalUnassignSave\") }} </slot>\r\n          <p></p>\r\n          <section class=\"my_radios\">\r\n            <div class=\"form-check\">\r\n              <input\r\n                @change=\"handleChange($event)\"\r\n                class=\"form-check-input\"\r\n                v-model=\"option\"\r\n                type=\"radio\"\r\n                value=\"1\"\r\n                id=\"option1\"\r\n              />\r\n              <label\r\n                class=\"form-check-label\"\r\n                for=\"flexRadioDefault1\"\r\n                style=\"color: red\"\r\n              >\r\n                <strong>{{ translate(\"modalNotifTrue\") }}</strong>\r\n              </label>\r\n            </div>\r\n            <div class=\"form-check\">\r\n              <input\r\n                @change=\"handleChange($event)\"\r\n                class=\"form-check-input\"\r\n                v-model=\"option\"\r\n                type=\"radio\"\r\n                value=\"2\"\r\n                id=\"option2\"\r\n              />\r\n              <label\r\n                class=\"form-check-label\"\r\n                for=\"flexRadioDefault2\"\r\n                style=\"color: red\"\r\n              >\r\n                <strong>{{ translate(\"modalNotifFalse\") }}</strong>\r\n              </label>\r\n            </div>\r\n          </section>\r\n        </section>\r\n\r\n        <footer class=\"modal-footer\">\r\n          <div class=\"row\">\r\n            <div class=\"col-md-6\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-danger\"\r\n                @click=\"unassign\"\r\n                aria-label=\"Close modal\"\r\n              >\r\n                <i class=\"fas fa-check me-1 act-btn\" aria-hidden=\"true\"></i>\r\n                {{ translate(\"btnUnassignModal\") }}\r\n              </button>\r\n            </div>\r\n            <div class=\"col-md-6\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-success\"\r\n                @click=\"close\"\r\n                aria-label=\"Close modal\"\r\n              >\r\n                <i\r\n                  class=\"fas fa-arrow-left me-1 act-btn\"\r\n                  aria-hidden=\"true\"\r\n                ></i>\r\n                {{ translate(\"btnModalBack\") }}\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </footer>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n    \r\n<style>\r\n.modal-backdrop {\r\n  position: fixed;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  background-color: rgba(0, 0, 0, 0.3);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal {\r\n  background: #ffffff;\r\n  border-radius: 6px;\r\n  box-shadow: 2px 2px 20px 1px;\r\n  overflow-x: hidden;\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 40%;\r\n  height: 35%;\r\n  position: absolute;\r\n  left: 50%;\r\n  top: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n\r\n.modal-header,\r\n.modal-footer {\r\n  padding: 15px;\r\n  display: flex;\r\n}\r\n\r\n.modal-header {\r\n  position: relative;\r\n  border-bottom: 1px solid #eeeeee;\r\n  color: #4aae9b;\r\n  justify-content: space-between;\r\n}\r\n\r\n.modal-footer {\r\n  border-top: 1px solid #eeeeee;\r\n  flex-direction: column;\r\n}\r\n\r\n.modal-body {\r\n  position: relative;\r\n  padding: 20px 10px;\r\n}\r\n\r\n.btn {\r\n  width: 150px;\r\n}\r\n\r\n.btn-close {\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  border: none;\r\n  font-size: 20px;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n  font-weight: bold;\r\n  color: #4aae9b;\r\n  background: transparent;\r\n}\r\n\r\n.btn-green {\r\n  color: white;\r\n  background: #4aae9b;\r\n  border: 1px solid #4aae9b;\r\n  border-radius: 2px;\r\n}\r\n\r\n.modal-fade-enter,\r\n.modal-fade-leave-to {\r\n  opacity: 0;\r\n}\r\n\r\n.modal-fade-enter-active,\r\n.modal-fade-leave-active {\r\n  transition: opacity 0.5s ease;\r\n}\r\n</style>\r\n    \r\n<script>\r\n  import en from \"../assets/en.js\";\r\n  import pt from \"../assets/pt.js\";\r\n  export default {\r\n    name: \"Modal\",\r\n    mixins: [en, pt],\r\n    data: function () {\r\n      const lang = localStorage.getItem(\"lang\") || \"pt\";\r\n      return {\r\n        lang: lang,\r\n        option: \"\",\r\n      };\r\n    },\r\n    methods: {\r\n      close() {\r\n        this.$emit(\"close\");\r\n      },\r\n      unassign() {\r\n        //this.$emit(\"unassign\");\r\n        this.option = alert(this.option);\r\n      },\r\n      translate(prop) {\r\n        return this[this.lang][prop];\r\n      },\r\n      handleChange(event) {\r\n        if (event.target.value == \"1\") this.option = true;\r\n        if (event.target.value == \"2\") this.option = false;\r\n      },\r\n    },\r\n  };\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}