dal-fade-leave-to {\r\n  opacity: 0;\r\n}\r\n\r\n.modal-fade-enter-active,\r\n.modal-fade-leave-active {\r\n  transition: opacity 0.5s ease;\r\n}\r\n</style>\r\n  \r\n  <script>\r\n  import en from \"../assets/en.js\";\r\n  import pt from \"../assets/pt.js\";\r\n  import { mapGetters } from \"vuex\";\r\n  import { IS_USER_AUTHENTICATED_GETTER } from \"../store/storeconstants\";\r\n  export default {\r\n    name: \"Modal\",\r\n    mixins: [en, pt],\r\n    props: [\"detailID\"],\r\n    data() {\r\n      const lang = localStorage.getItem(\"lang\") || \"pt\";\r\n      return {\r\n        lang: lang,\r\n        form: {\r\n          firstname: \"\",\r\n          lastname: \"\",\r\n          name: \"\",\r\n          mobile: \"\",\r\n          type: \"\",\r\n          email: \"\",\r\n          mobile: \"\",\r\n          bdate: \"\",\r\n          notifications: false,\r\n        },\r\n      };\r\n    },\r\n    computed: {\r\n      ...mapGetters(\"auth\", {\r\n        isAuthenticated: IS_USER_AUTHENTICATED_GETTER,\r\n        auth: localStorage.getItem(\"auth\"),\r\n      }),\r\n    },\r\n    methods: {\r\n      close() {\r\n        this.$emit(\"close\");\r\n      },\r\n      translate(prop) {\r\n        return this[this.lang][prop];\r\n      },\r\n      async getDetail(id) {\r\n        (this.isShow = true((this.message.type = \"\"))), (this.message.msg = \"\");\r\n        await axios\r\n          .get(\"https://bhsapi.duartecota.com/user/\" + id, {\r\n            headers: {\r\n              Authorization: this.token,\r\n            },\r\n          })\r\n          .then((response) => {\r\n            (this.form.firstname = response.data.body.firstname),\r\n              (this.form.lastname = response.data.body.lastname),\r\n              (this.form.name = response.data.body.name),\r\n              (this.form.type = response.data.body.type),\r\n              (this.form.email = response.data.body.email),\r\n              (this.form.bdate = response.data.body.bdate),\r\n              (this.form.mobile = response.data.body.mobile);\r\n            if (response.data.body.notifications == true)\r\n              this.form.notifications = \"SIM\";\r\n            else this.form.notifications = \"NÃƒO\";\r\n            //this.state = true;\r\n          })\r\n          .catch(() => {\r\n            this.message.msg = \"Ocorreu um problema\";\r\n            this.message.type = \"warning\";\r\n            this.isShow = false;\r\n          });\r\n      },\r\n    },\r\n  };\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}