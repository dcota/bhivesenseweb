{"ast":null,"code":"import en from \"../assets/en.js\";\nimport pt from \"../assets/pt.js\";\nimport { mapGetters } from \"vuex\";\nimport { IS_USER_AUTHENTICATED_GETTER } from \"../store/storeconstants\";\nexport default {\n  name: \"Modal\",\n  mixins: [en, pt],\n  props: [\"description\", \"observations\", \"intervention\"],\n  data() {\n    const lang = localStorage.getItem(\"lang\") || \"pt\";\n    return {\n      lang: lang,\n      altImg: require(\"../assets/avatar.png\")\n    };\n  },\n  computed: {\n    ...mapGetters(\"auth\", {\n      isAuthenticated: IS_USER_AUTHENTICATED_GETTER,\n      auth: localStorage.getItem(\"auth\")\n    })\n  },\n  methods: {\n    close() {\n      this.$emit(\"close\");\n    },\n    edit() {\n      this.$emit(\"edit\");\n    },\n    translate(prop) {\n      return this[this.lang][prop];\n    }\n  }\n};","map":{"version":3,"mappings":"AAkEE,OAAOA,EAAC,MAAO,iBAAiB;AAChC,OAAOC,EAAC,MAAO,iBAAiB;AAChC,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,4BAA2B,QAAS,yBAAyB;AACtE,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,MAAM,EAAE,CAACL,EAAE,EAAEC,EAAE,CAAC;EAChBK,KAAK,EAAE,CAAC,aAAa,EAAE,cAAc,EAAE,cAAc,CAAC;EACtDC,IAAI,GAAG;IACL,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAO,CAAC,MAAM,KAAK,IAAI;IACjD,OAAO;MACLF,IAAI,EAAEA,IAAI;MACVG,MAAM,EAAEC,OAAO,CAAC,sBAAsB;IACxC,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGX,UAAU,CAAC,MAAM,EAAE;MACpBY,eAAe,EAAEX,4BAA4B;MAC7CY,IAAI,EAAEN,YAAY,CAACC,OAAO,CAAC,MAAM;IACnC,CAAC;EACH,CAAC;EACDM,OAAO,EAAE;IACPC,KAAK,GAAG;MACN,IAAI,CAACC,KAAK,CAAC,OAAO,CAAC;IACrB,CAAC;IACDC,IAAI,GAAG;MACL,IAAI,CAACD,KAAK,CAAC,MAAM,CAAC;IACpB,CAAC;IACDE,SAAS,CAACC,IAAI,EAAE;MACd,OAAO,IAAI,CAAC,IAAI,CAACb,IAAI,CAAC,CAACa,IAAI,CAAC;IAC9B;EACF;AACF,CAAC","names":["en","pt","mapGetters","IS_USER_AUTHENTICATED_GETTER","name","mixins","props","data","lang","localStorage","getItem","altImg","require","computed","isAuthenticated","auth","methods","close","$emit","edit","translate","prop"],"sourceRoot":"","sources":["C:\\Users\\duart\\OneDrive - PGA\\Ambiente de Trabalho\\bhivesens