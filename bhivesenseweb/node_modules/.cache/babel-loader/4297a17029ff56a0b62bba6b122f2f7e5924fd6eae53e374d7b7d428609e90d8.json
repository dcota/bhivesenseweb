{"ast":null,"code":"import en from \"../assets/en.js\";\nimport pt from \"../assets/pt.js\";\nimport { mapGetters } from \"vuex\";\nimport { IS_USER_AUTHENTICATED_GETTER } from \"../store/storeconstants\";\nexport default {\n  name: \"Modal\",\n  mixins: [en, pt],\n  props: [\"description\", \"observations\", \"type\", \"color\", \"details\"],\n  data() {\n    const lang = localStorage.getItem(\"lang\") || \"pt\";\n    return {\n      lang: lang,\n      altImg: require(\"../assets/avatar.png\")\n    };\n  },\n  computed: {\n    ...mapGetters(\"auth\", {\n      isAuthenticated: IS_USER_AUTHENTICATED_GETTER,\n      auth: localStorage.getItem(\"auth\")\n    })\n  },\n  mounted() {},\n  methods: {\n    close() {\n      this.$emit(\"close\");\n    },\n    edit(id) {\n      localStorage.setItem(\"interventiontoedit\", id);\n      this.$emit(\"edit\");\n    },\n    done() {\n      localStorage.setItem(\"interventiontoedit\", id);\n      this.$emit(\"done\");\n    },\n    cancel() {\n      localStorage.setItem(\"interventiontoedit\", id);\n      this.$emit(\"delete\");\n    },\n    translate(prop) {\n      return this[this.lang][prop];\n    }\n  }\n};","map":{"version":3,"mappings":"AAgLE,OAAOA,EAAC,MAAO,iBAAiB;AAChC,OAAOC,EAAC,MAAO,iBAAiB;AAChC,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,4BAA2B,QAAS,yBAAyB;AACtE,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,MAAM,EAAE,CAACL,EAAE,EAAEC,EAAE,CAAC;EAChBK,KAAK,EAAE,CAAC,aAAa,EAAE,cAAc,EAAE,MAAM,EAAE,OAAO,EAAE,SAAS,CAAC;EAClEC,IAAI,GAAG;IACL,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAO,CAAC,MAAM,KAAK,IAAI;IACjD,OAAO;MACLF,IAAI,EAAEA,IAAI;MACVG,MAAM,EAAEC,OAAO,CAAC,sBAAsB;IACxC,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGX,UAAU,CAAC,MAAM,EAAE;MACpBY,eAAe,EAAEX,4BAA4B;MAC7CY,IAAI,EAAEN,YAAY,CAACC,OAAO,CAAC,MAAM;IACnC,CAAC;EACH,CAAC;EACDM,OAAO,GAAG,CAAC,CAAC;EACZC,OAAO,EAAE;IACPC,KAAK,GAAG;MACN,IAAI,CAACC,KAAK,CAAC,OAAO,CAAC;IACrB,CAAC;IACDC,IAAI,CAACC,EAAE,EAAE;MACPZ,YAAY,CAACa,OAAO,CAAC,oBAAoB,EAAED,EAAE,CAAC;MAC9C,IAAI,CAACF,KAAK,CAAC,MAAM,CAAC;IACpB,CAAC;IACDI,IAAI,GA