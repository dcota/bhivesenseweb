ER_LEVEL_GETTER,\r\n        _id: GET_USER_ID_GETTER,\r\n      }),\r\n    },\r\n    mounted() {\r\n      this.getApiaries();\r\n    },\r\n    methods: {\r\n      async getApiaries() {\r\n        this.message.type = \"\";\r\n        this.message.msg = \"\";\r\n        this.isShow = true;\r\n        this.apiaries = [];\r\n        await axios\r\n          .get(\"https://bhsapi.duartecota.com/apiary/\" + this._id, {\r\n            headers: {\r\n              Authorization: this.token,\r\n            },\r\n          })\r\n          .then((response) => {\r\n            console.log(response.data.body);\r\n            let apiaries = response.data.body;\r\n            for (let i = 0; i < apiaries.length; i++) {\r\n              //if (users[i].level == \"beekeeper\") {\r\n              this.apiaries.push({\r\n                _id: apiaries[i]._id,\r\n                num: i + 1,\r\n                address: apiaries[i].address,\r\n                location: apiaries[i].location,\r\n                observations: apiaries[i].observations,\r\n              });\r\n              // }\r\n            }\r\n            this.isShow = false;\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n            this.isShow = false;\r\n            this.showsection = true;\r\n            this.message.type = \"danger\";\r\n            this.message.msg = this.translate(\"mesProblem\");\r\n            this.isShow = true;\r\n          });\r\n      },\r\n      translate(prop) {\r\n        return this[this.lang][prop];\r\n      },\r\n    },\r\n  };\r\n</script>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}