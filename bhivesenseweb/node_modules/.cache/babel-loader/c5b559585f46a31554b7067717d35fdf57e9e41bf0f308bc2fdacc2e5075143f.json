 localStorage.getItem(\"auth\"),\r\n      }),\r\n    },\r\n    mounted() {},\r\n    methods: {\r\n      close() {\r\n        this.$emit(\"close\");\r\n      },\r\n      edit() {\r\n        this.$emit(\"edit\");\r\n      },\r\n      done() {\r\n        this.$emit(\"done\");\r\n      },\r\n      cancel() {\r\n        this.$emit(\"delete\");\r\n      },\r\n      translate(prop) {\r\n        return this[this.lang][prop];\r\n      },\r\n    },\r\n  };\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}